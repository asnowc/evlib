  "exports": {
    ".": "./dist/core/mod.js",
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@eavid/lib-dev": "^0.0.3",
    "@microsoft/api-extractor": "^7.43.1",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "keywords": [],
  "engines": {
    "pnpm": ">=9"
  }
