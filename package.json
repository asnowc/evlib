{
  "name": "evlib",
  "version": "2.4.0",
  "description": "",
  "type": "module",
  "author": "Eavid",
  "license": "MIT",
  "scripts": {
    "build": "pnpm rollup -c",
    "publish:try": "pnpm publish --no-git-checks --dry-run",
    "ci:build": "pnpm build",
    "ci:api-extractor": "api-extractor run -c api-extractor.jsonc -v",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --ui",
    "ci:test": "vitest run",
    "api:extractor-local": "pnpm ci:api-extractor -l"
