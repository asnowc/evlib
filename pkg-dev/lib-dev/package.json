        },
        "./rollup/*": "./out/rollup/*.js"
    },
    "author": "Eavid",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.5.7"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-typescript": "^11.1.4",
        "glob": "^10.3.4",
        "rollup": "^3.29.4",
        "rollup-plugin-ts-paths": "^1.0.5"
    },
    "files": [
        "out/**/*.js",
        "out/**/*.d.ts"
    ],
    "publishConfig": {
