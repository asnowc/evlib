{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@eavid/lib-node/*": ["./src/*.ts"], // 导入包名提示
      "@src/*": ["./src/*"],
      "#evlib": ["evlib"] //方便复制模块
    },
    "lib": ["ESNext"],
    "allowJs": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "out", "dist"]
}
